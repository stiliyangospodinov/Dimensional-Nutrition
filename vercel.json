{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "myproject/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "myproject/dist/index.html"
      }
    ]
  }
  